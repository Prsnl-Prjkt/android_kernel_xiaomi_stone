&qcom {
	/delete-property/ mem-type;
	/delete-property/ console-size;
	record-size = <0x100000>;
	pmsg-size = <0x100000>;
	max-reason = <4>;
};

&ufshc_mem {
	spm-level = <3>;
};

&sdhc_2 {
	cd-gpios = <&tlmm 94 GPIO_ACTIVE_HIGH>;
};

&soc {
	qcom,glink {
		glink_adsp: adsp {
				qcom,adsp_qrtr {
						qcom,non-wake-svc = <0x190>;
						};
				};
		};
	jtag_mm0: jtagmm@9040000 {
		status = "disabled";
	};
	jtag_mm1: jtagmm@9140000 {
		status = "disabled";
	};
	jtag_mm2: jtagmm@9240000 {
		status = "disabled";
	};
	jtag_mm3: jtagmm@9340000 {
		status = "disabled";
	};
	jtag_mm4: jtagmm@9440000 {
		status = "disabled";
	};
	jtag_mm5: jtagmm@9540000 {
		status = "disabled";
	};
	jtag_mm6: jtagmm@9640000 {
		status = "disabled";
	};
	jtag_mm7: jtagmm@9740000 {
		status = "disabled";
	};
};

